idContext=item.context.some(ctx=>content.includes(ctx))||QQ_IsValidMultiPersonContext(content,item.keyword);if(hasValidContext){return true;}else{}}}const characterMentions=content.match(/[@ã€ï¼ˆï¼œ]\s*([^@ã€ï¼ˆï¼œã€‘ï¼‰ï¼ž\s]{2,8})\s*[ã€‘ï¼‰ï¼ž]/g)||[];const uniqueCharacters=[...new Set(characterMentions.map(m=>m.replace(/[@ã€ï¼ˆï¼œã€‘ï¼‰ï¼ž]/g,'').trim()))];if(primaryCharacter&&uniqueCharacters.length>=1){const isPrimaryDominant=QQ_IsPrimaryCharacterDominant(content,primaryCharacter,uniqueCharacters);if(isPrimaryDominant){return false;}}if(uniqueCharacters.length>1){return true;}const strongMultiPersonPatterns=[/(\w+)å’Œ(\w+)(?:ä»¥åŠ|è¿˜æœ‰|åŠ ä¸Š)(\w+)/,/(\w+)ã€(\w+)(?:ã€(\w+))+/,/(?:å’Œ|ä¸Ž|è·Ÿ)(\w+)(?:ä»–ä»¬|å¥¹ä»¬|å¤§å®¶)(?:ä¸€èµ·|åŒæ—¶)/,/(?:æˆ‘ä»¬|å’±ä»¬)(?:å‡ ä¸ª|å¤šä¸ª|å…¨éƒ¨)(?:ä¸€èµ·|åŒæ—¶)/];if(strongMultiPersonPatterns.some(pattern=>pattern.test(content))){return true;}return false;}function QQ_IsValidMultiPersonContext(content,keyword){const invalidContexts=[/é›†ä½“è¡ŒåŠ¨/,/é›†ä½“æ´»åŠ¨/,/é›†ä½“å†³ç­–/,/é›†ä½“åˆ©ç›Š/,/é›†ä½“æ„è¯†/,/é›†ä½“è®°å¿†/,/æ¦‚å¿µä¸Š/,/ç†è®ºä¸Š/,/åŽŸåˆ™ä¸Š/,/æ€æƒ³ä¸Š/,/ç²¾ç¥žä¸Š/,/æ›¾ç».*ä¸€èµ·/,/ä»¥å‰.*ä¸€èµ·/,/ä¹‹å‰.*ä¸€èµ·/,/è¿‡åŽ».*ä¸€èµ·/,/å¦‚æžœ.*ä¸€èµ·/,/å‡å¦‚.*ä¸€èµ·/,/è¦æ˜¯.*ä¸€èµ·/,/å€˜è‹¥.*ä¸€èµ·/];if(invalidContexts.some(pattern=>pattern.test(content))){return false;}const interactiveVerbs=[/ä¸€èµ·(åŽ»|æ¥|åš|çŽ©|èŠ|è®¨è®º|å‚ä¸Ž|è¿›è¡Œ)/,/(å¯¹|å’Œ|è·Ÿ).*(è¯´|è®²|èŠ|äº¤æµ)/,/(æŠ±|æ‹¥æŠ±|äº²å»|æ¡æ‰‹|æ‹|ç¢°|æ‘¸)/,/(çœ‹|æœ›|æ³¨è§†|å‡è§†).*(ç€|å‘)/,/(èµ°å‘|èµ°è¿‘|é è¿‘|æŽ¥è¿‘)/];const hasInteractiveVerb=interactiveVerbs.some(pattern=>pattern.test(content));const currentTimeIndicators=[/çŽ°åœ¨/,/æ­¤åˆ»/,/å½“å‰/,/é©¬ä¸Š/,/ç«‹å³/,/æ­£åœ¨/,/è¦.*ä¸€èµ·/,/æƒ³.*ä¸€èµ·/,/å‡†å¤‡.*ä¸€èµ·/];const hasCurrentTime=currentTimeIndicators.some(pattern=>pattern.test(content));return hasInteractiveVerb||hasCurrentTime;}function QQ_IsPrimaryCharacterDominant(content,primaryCharacter,allCharacters){let primaryWeight=0;let otherWeight=0;const primaryCount=(content.match(new RegExp(primaryCharacter,'g'))||[]).length;const otherCounts=allCharacters
.filter(char=>char!==primaryCharacter).reduce((sum,char)=>sum+(content.match(new RegExp(char,'g'))||[]).length,0);primaryWeight+=primaryCount*2;otherWeight+=otherCounts;const interactionPatterns=[new RegExp(`${primaryCharacter}(?:èµ°å‘|èµ°è¿‘|æŠ±ä½|æ‹‰ä½|çœ‹ç€|å¯¹.*è¯´)`,'g'),new RegExp(`(?:èµ°å‘|èµ°è¿‘|æŠ±ä½|æ‹‰ä½|çœ‹ç€|å¯¹)${primaryCharacter}`,'g'),];interactionPatterns.forEach(pattern=>{const matches=content.match(pattern)||[];primaryWeight+=matches.length*3;});const directDialogPatterns=[new RegExp(`${primaryCharacter}[è¯´é“è®²]`,'g'),new RegExp(`å¯¹${primaryCharacter}è¯´`,'g'),];directDialogPatterns.forEach(pattern=>{const matches=content.match(pattern)||[];primaryWeight+=matches.length*2;});const intimatePatterns=[new RegExp(`(?:å’Œ|ä¸Ž|è·Ÿ)${primaryCharacter}(?:å•ç‹¬|ç§ä¸‹|æ‚„æ‚„|å·å·)`,'g'),new RegExp(`${primaryCharacter}(?:å•ç‹¬|ç§ä¸‹|æ‚„æ‚„|å·å·)`,'g'),];intimatePatterns.forEach(pattern=>{const matches=content.match(pattern)||[];primaryWeight+=matches.length*4;});return primaryWeight>otherWeight*0.6;}function QQ_ShowPublicInteractiveSpace(content,characterName=""){const spaceId=QQ_GenerateSpaceId();const spaceName=QQ_GetSpaceName(content,characterName);const charName=characterName||charcardname||"è§’è‰²";const charAvatar=charAvatarPath||"default_avatar.png";QQ_InteractiveSpaces[spaceId]={id:spaceId,name:spaceName,content:content,characterName:charName,characterAvatar:charAvatar,type:'public',participants:QQ_ExtractParticipants(content),createdAt:new Date().toISOString(),lastAccess:new Date().toISOString()};QQ_ActiveSpaceId=spaceId;QQ_SaveInteractiveSpaces().then(()=>{}).catch(error=>{console.error('âŒ å…¬å…±äº’åŠ¨ç©ºé—´ä¿å­˜å¤±è´¥:',error);});QQ_DisplayInteractiveSpace(spaceId,'å…¬å…±äº’åŠ¨ç©ºé—´');}async function QQ_ShowCharacterInteractiveSpace(content,characterName){const charName=characterName||charcardname||"è§’è‰²";await QQ_ForceReloadInteractiveSpaces();const spaceId=QQ_GenerateSpaceId();const spaceName=`${charName}Â·${QQ_GetSceneFromContent(content